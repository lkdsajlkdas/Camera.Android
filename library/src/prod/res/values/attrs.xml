<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CameraView">
        <!-- The recognition mode. -->
        <attr name="recognition" format="enum">
            <!-- default mode -->
            <enum name="none" value="0"/>
            <!-- Enabled barcode mode. -->
            <enum name="barcode" value="1"/>
        </attr>
    </declare-styleable>
</resources>
